{
    "name": "RoundLogExtension",
    "version": "1.1.4",
    "manifest_version": 2,
    "browser_action": {
    	"default_title": "Round Log Extension",
    	"default_popup": "popup.html",
    	"default_icon": {
        	"19": "icons/19x19.png",
        	"38": "icons/38x38.png",
        	"48": "icons/48x48.png",
        	"128": "icons/128x128.png"
    	}
    },

    "background": {
	    "scripts": ["background.js"],
    	"persistent": true
	},

	"chrome_url_overrides" : {
    	"newtab": "newtab.html"
	},

	"permissions": [
		"notifications"
	],

	"web_accessible_resources": [
		"48x48.png"
	],

	"options_page": "options.html"

}